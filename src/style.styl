.owner_id
    border solid 1px blue
    width 20em
    margin 1rem
    padding 1rem
    display inline-block
    height 1em

disable-user-select()
    -webkit-touch-callout none
    -webkit-user-select   none
    -khtml-user-select    none
    -moz-user-select      none
    -ms-user-select       none
    user-select           none
    cursor             default

.button
    border solid 1px green
    margin 1rem
    padding 1rem
    display inline-block
    disable-user-select()
    height 1em

.button:hover
    background-color rgba(0, 255, 0, 0.1)


